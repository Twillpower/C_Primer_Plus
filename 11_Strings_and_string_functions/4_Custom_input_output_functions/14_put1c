/* put1.c -- 打印字符串，不添加\n */
#include <stdio.h>

void put1(const char * string)/* 不会改变字符串 */
{
    while (*string != '\0')
        putchar(*string++);
}

void put1_1(const char * string)
{
    int i = 0;
    while (string != '\0')  // 或 while (*string)
    {
        putchar(string[i++]);
    }
    
}